   "source": [
    "import numpy as np\n",
    "\n",
    "def softmax(x):\n",
    "    C = np.max(x)\n",
    "    tmp = np.exp(x-C)\n",
    "    return tmp/np.sum(tmp)\n",
    "\n",
    "a = np.array([0.3, 2.9, 4.0])\n",
    "y = softmax(a)\n",
    "print(y)"
   ]
  }
